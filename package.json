{
  "name": "jdue-cfv",
  "version": "1.0.2",
  "private": true,
  "description": "Full-stack to-do application for Cloudflare Pages (CFV of JDue)",
  "scripts": {
  "dev": "vite",
  "build": "VITE_APP_VERSION=$(git describe --tags --abbrev=0) tsc && vite build",
  "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
  "preview": "vite preview",
  "pages:dev": "wrangler pages dev . --d1=jdue-cfv-db",
  "pages:deploy": "wrangler pages deploy . --commit-dirty=true",
  "db:migrate": "wrangler d1 execute jdue-cfv-db --file=./schema.sql"
  },
  "dependencies": {
    "@tsndr/cloudflare-worker-jwt": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "itty-router": "^4.0.23",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "wrangler": "^3.60.0"
  }
}